    <div class="section10">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="d-flex align-items-lg-center flex-lg-row flex-column">
                        <div class="burron">
                            <div>
                                <div>
                                <a href="/product_details" > <button type="button" class="btn btn-soft-success shadow-none"><i class="ri-add-circle-line align-middle me-1"></i>Product List</button></a>
                            </div>
                            </div> 
                            <div>
                                <form class="product_class" action="/user_list" method="get" >  
                                    <button type="submit" class="btn btn-soft-success shadow-none"><i class="ri-pulse-line"></i>Users</button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
