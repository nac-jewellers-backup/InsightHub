    <div id="layout-wrapper">     
      
        <div class="main-content">           
            <div class="page-content new-style">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="card">
                                <img src="admin/public/assets/naclogo.jpg" class="myClass" alt="">

                                
                                <div class="card-header">                                   
                                    <h4 class="card-title mb-0 flex-grow-1" style="font-weight: bold;">Bidding Details</h4>
                                </div>                               
                                <div class="card-body overflowx">
                                    <div class="row mb-2 pl-4">
                                        <div class="col-3">
                                            <label for="dropdown1">Product List</label>
                                            <select class="form-control"  id="dropdown1">
                                                <option value="">All</option>
                                                <%for(var c=0; c < count_ba.length; c++){%>
                                                <option value="<%-count_ba[c].Item_Name%>"><%-count_ba[c].Item_Name%></option>
                                                <%}%>
                                            </select> 
                                        </div>
                                        <div class="col-3">
                                            <label for="dropdown2">Product List</label>
                                            <select class="form-control"  id="dropdown2">
                                                <option value="">All</option>
                                                <option value="Unsuccessful">Unsuccessful</option>
                                                <option value="Successful">Successful</option>                                                    
                                            </select>  
                                        </div>
                                    </div>                    
                                    <table id="example1" class="table table-bordered dt-responsive nowrap table-striped" style="width:100%">
                                        
                                        <thead>
                                            <tr class="table-header">                                              
                                                <th class="cell">Bidding Product</th>  
                                                <th class="cell">Bidding Amount</th>  
                                                <th class="cell">Bidding Status</th> 
                                                <th class="cell">Bidding Time</th>                                                                                                  
                                            </tr>
                                        </thead>
                                        <tbody>
                                            
                                            <%
                                            var b={};
                                                user_list.forEach(el => {
                                                    b[el.item_name] = (b[el.item_name] || 0) + 1;
                                                })                   
                                                console.log(b)  
                                            for(var t=0; t < user_list.length; t++){%>        
                                                                           
                                        
                                                <tr class="textcenter">
                                                    <td colspan="<%-b%>"><%-user_list[t].item_name%></td>                                         
                                                    <td style="text-align: right;"><%-(user_list[t].bid_amount) ? parseFloat(user_list[t].bid_amount).toLocaleString('en-IN', {maximumFractionDigits: 2, style: 'currency',currency: 'INR'}): "-"%></td>
                                                        <%
                                                            var status = '';
                                                            var colour = '';

                                                            if(user_list[t].status == 0){
                                                                status="Unsuccessful";
                                                                color = 'danger';
                                                            } else {
                                                                status= "Successful"
                                                                color='success'
                                                            }
                                                        %>
                                                        <td><span class="badge bg-<%-color%>"><%-status%></td>
                                                        <%
                                                            var betting_date = user_list[t].new_created_date
                                                            if(!betting_date){betting_date="Dint Bid Yet"}
                                                            else{ betting_date= moment(user_list[t].new_created_date).format('hh:mm A')}
                                                        %>
                                                    <td> <%-betting_date%></td>
                                                 </tr>
                                            <%}%>                                       
                                        </tbody>
                                    </table>
                                    <!-- Edit Modal HTML -->
                                    
                                    <!-- end modal --> 
                                    <!-- create Modal HTML -->
                                    
                                    <!-- end modal --> 
