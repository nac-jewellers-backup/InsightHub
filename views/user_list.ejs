<script>
    $(document).ready(function () {
        $('#example1').DataTable({
        order: [[0, 'desc']],
        "autoWidth": false
    });
});
</script>
<script>
        $(document).ready(function() {
    var table = $('#example1').DataTable();

   
    $('#dropdown1').change(function() {
        var selectedValue = $(this).val();
        var searchValue;

        if (selectedValue === '0') {
        searchValue = '^(0)$';
        } else if (selectedValue === '1') {
        searchValue = '^(?!0$)';
        } else {
        searchValue = ''; 
        }

        // Clear previous search
        table.column(8).search('');

        // Apply the new search
        table.column(8).search(searchValue, true, false);

        // Trigger the table search
        table.draw();
    });
    });
</script>

