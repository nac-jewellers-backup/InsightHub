    <div id="layout-wrapper">             
             
        <div class="main-content">           
            <div class="page-content new-style">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="card">
                                <div class="card-header">
                                   
                                    <h4 class="card-title mb-0 flex-grow-1" style="font-weight: bold;">User List</h4>
                                </div>
                                
                                <div style="width: 20% !important;">
                                    <select class="form-control"  id="dropdown1">
                                        <option value="">All</option>
                                      
                                        <option value="0">No of users not Bid</option>
                                        <option value="1">No of users Bid</option>
                                      
                                    </select>                                              
                                </div>
                                <div class="card-body overflowx">
                                
            
                                    <table id="example1" class="table table-bordered dt-responsive table-striped" style="width:100%">
                                        <thead>
                                            <tr class="table-header">
                                                <th class="cell">User Id</th>
                                                <th class="cell">User Name</th>
                                                <th class="cell">Email</th>
                                                <th class="cell">Mobile</th>
                                                <th class="cell"  style="width:300px !important;">Location</th>                                                
                                                <th class="cell">Registered Date</th>                                                                                                                            
                                                <th class="cell">No of time login</th> 
                                                <th class="cell">Last Login</th> 
                                                <th class="cell">No of Bids</th>     
                                                <th class="cell">Highest Bid Value</th>    
                                                <th class="cell">Bidding Details</th>                                              
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <%for(var t=0; t < user_list.length; t++){%>
                                                <tr class="textcenter">                       
                                                    <!-- <td class="my-tdfirst text-center"><%=t+1%></td>                           -->
                                                    <td><%-user_list[t].user_id%></td>
                                                    <td style="text-align: left;"><%-user_list[t].user_name%></td>                                         
                                                    <td style="text-align: left;"><%-user_list[t].email%></td>
                                                    <td><%-user_list[t].mobile%></td>
                                                    <td style="text-align: left;"><%-(user_list[t].location).substring(0,43)%>...</td>      
                                                      
                                                    
                                                    <td><%=moment(user_list[t].new_created_date).format('DD-MM-YYYY hh:mm A')%></td>
                                                        <%
                                                            var betting_date = user_list[t].betting_created_date
                                                            if(!betting_date){
                                                                betting_date="Dint Bid Yet"
                                                                }
                                                                else{
                                                                    betting_date= moment(user_list[t].betting_created_date).format('DD-MM-YYYY')
                                                                }
                                                        %>
                                                    <!-- <td> <%-betting_date%></td> -->
                                                        <%
                                                            var betting_amount = user_list[t].bit_amount
                                                            if(!betting_amount){
                                                                betting_amount="Dint Bid Yet"
                                                                }
                                                                else{
                                                                    betting_amount= user_list[t].bit_amount
                                                                }
                                                        %>
                                                    <!-- <td><%-betting_amount%></td> -->
                                                        <%
                                                            var logincount = user_list[t].login_count
                                                            if(isNaN(logincount)){
                                                                    logincount=0
                                                                }
                                                        %>
                                                    <td style="text-align: right;">
                                                        <%-logincount%>
                                                    </td>
                                                        <%
                                                            var last_login = user_list[t].new_login
                                                            if(!last_login){
                                                                last_login="-"
                                                                }
                                                                else{
                                                                    last_login= moment(user_list[t].new_login).format('DD-MM-YYYY hh:mm A')
                                                                }
                                                        %>
                                                    <td><%=last_login%></td> 
                                                    <td style="text-align: right;"><%-user_list[t].total_bid%></td>
                                                    <td style="text-align: right;"><%-(user_list[t].max_bid) ? parseFloat(user_list[t].max_bid).toLocaleString('en-IN', {maximumFractionDigits: 2, style: 'currency',currency: 'INR'}): "-"%></td>
                                                    <td>
                                                        <div class="dropdown d-inline-block">
                                                                
                                                                
                                                                
                                                                 <form action="/bidding_details">
                                                                    <input type="hidden" name="user_id" value="<%=user_list[t].user_id%>"><button class="btn btn-soft-secondary btn-sm " type="submit" title="Edit" >
                                                                    <i class="ri-eye-line align-middle"></i>
                                                                </button>
                                                            </form>
                                                                                                                    
                                                        </div>
                                                    </td> 
                                                </tr>
                                            <%}%>
                                        </tbody>
                                    </table>
